<html>
<head>
<meta charset="UTF-8">
<meta name="viewport"
	content="width=device-width,initial-scale = 1.0" />
<title>Insert title here</title>

</head>
<body>

	<div onmouseover="mOver(this)" onmouseout="mOut(this)"
		style="background-color: red; width: 120px; height: 20px; padding: 40px;font-size: large; font-weight: bold;">Mouse
		Over Me</div>

<script>
function mOver(obj){
    obj.innerHTML = "Thank you"
}

function mOut(obj){
    obj.innerHTML = "Mouse Over Me"
}
</script>

</body>
</html>